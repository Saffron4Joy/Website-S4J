source "https://rubygems.org"

# Jekyll 3.9 works with GitHub Pages and Ruby 2.6+
gem "jekyll", "~> 3.9.0"
gem "webrick", "~> 1.7"

# Lock ffi to older version that works with Ruby 2.6
gem "ffi", "~> 1.15.0"

group :jekyll_plugins do
  gem "jekyll-paginate"
  gem "jekyll-seo-tag"
  gem "jekyll-sitemap"
  gem "jekyll-feed"
end
